{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/src/sw_clock/**",
                "${workspaceFolder}/src/kf_servo/**",
                "${workspaceFolder}/gtests/**",
                "/usr/include",
                "/usr/local/include"
            ],
            "defines": [
                "__APPLE__"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compileCommands": "${workspaceFolder}/build/debug-macos/compile_commands.json"
        }
    ],
    "version": 4
}